{
  "name": "wiki-result-table",
  "version": "1.0.0",
  "description": "Generates SA election result tables for Wikipedia",
  "main": "dist/index.js",
  "scripts": {
    "dev": "source environment && yarn nodemon -e js,ejs --exec babel-node src/index.js",
    "build": "yarn babel src/ -d dist/ --copy-files --ignore 'src/static/*.js'",
    "clean": "yarn rimraf dist/",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Adrian Frith <adrian@frith.dev>",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.13.4",
    "koa-ejs": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "winston": "^2.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2"
  }
}
